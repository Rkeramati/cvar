    "def estimate_cvar(dist, alpha, n=int(1e7)):\n",
    "    # Numerical Calculation of CVaR\n",
    "    rvs = np.hstack([dist.rvs(size=n)]) \n",
    "    x = np.sort(rvs)\n",
    "    estCVaR = np.sum(x[0: int(n*alpha)]) / (int(n*alpha) + 1)\n",
    "    return estCVaR"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [],
   "source": [
    "Normal_CVaR = estimate_cvar(Normal, alpha, n=int(1e7))\n",
    "TruncNormal_CVaR = estimate_cvar(TruncNormal, alpha, n=int(1e7))\n",
    "Bernoulli_CVaR = estimate_cvar(Bernoulli, alpha, n=int(1e7))"
   ]
